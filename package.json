{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "predev": "tsx scripts/kill-ports.js",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend:with-backend\"",
    "dev:backend": "tsx server/index.ts",
    "dev:frontend": "vite",
    "dev:frontend:with-backend": "tsx scripts/wait-for-backend.js && npm run dev:frontend",
    "build:frontend": "NODE_ENV=production vite build",
    "build:backend": "esbuild server/index.ts --platform=node --bundle --format=esm --outdir=dist --external:pg-native --external:fsevents --external:lightningcss --external:@babel/preset-typescript/package.json --external:esbuild",
    "build": "npm run build:frontend && npm run build:backend",
    "postbuild": "cp package.json dist/ && cd dist && npm install --omit=dev",
    "start": "cd dist && NODE_ENV=production node --experimental-modules index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push"
  },
